# platform :ios, '9.0'

target 'Yourself' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Yourself
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'BEMCheckBox'

  target 'YourselfTests' do
    inherit! :complete
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    pod 'GoogleSignIn'
    pod 'BEMCheckBox'
  end

end
